# Example file - copy to .env and fill with your values

# Nextcloud Calendar
NEXTCLOUD_USERNAME=your_username
NEXTCLOUD_PASSWORD=your_password
CALENDAR_URL=https://your.nextcloud.com/remote.php/dav/calendars/user/personal/?export

# Nextcloud Tasks (CalDAV endpoint)
TASKS_URL=https://your.nextcloud.com/remote.php/dav/calendars/user/personal

# Nextcloud Grocery List (CalDAV endpoint)
GROCERY_URL=https://your.nextcloud.com/remote.php/dav/calendars/user/spesa

# Weather API (https://openweathermap.org/api)
OPENWEATHER_API_KEY=your_api_key